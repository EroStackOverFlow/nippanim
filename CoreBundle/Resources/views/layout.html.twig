ction(event) {	    		var img = new Image();         		img.onload = function() {				var ratio = img.width / img.height;				var width = 300;				var height = Math.round(width / ratio);				var canvas = $('<canvas class="thumbnail thumbnail_4"></canvas>').attr({ width: width, height: height });				file.after(canvas);				var context = canvas[0].getContext('2d');	        	    	context.drawImage(img, 0, 0, width, height);			};         		img.src = event.target.result;    		};     		reader.readAsDataURL(file[0].files[0]);	});}); </script>		    {% endblock %}</body></html>